<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
	<changeSet id="best_game_db: tables" author="oromanov">
		<dropTable tableName="machine"/>
		<sqlFile path="/initial/tables/machine.sql" relativeToChangelogFile="true" />
		<rollback/>
	</changeSet>
	<changeSet id="best_game_db: data" author="oromanov">
		<sqlFile path="/initial/data/machine.sql" relativeToChangelogFile="true"/>
		<rollback/>
	</changeSet>
</databaseChangeLog>
